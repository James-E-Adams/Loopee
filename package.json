  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "autoprefixer": "^9.5.0",
    "classnames": "^2.2.6",
    "postcss-cli": "^6.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8",
    "react-social-icons": "^4.1.0",
    "recompose": "^0.30.0",
    "tailwindcss": "^0.7.4"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
